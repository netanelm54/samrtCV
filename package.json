{
  "name": "smartcv",
  "version": "1.0.0",
  "description": "AI-powered CV improvement service",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:staging": "concurrently \"cd frontend && npm run dev:staging\" \"cd backend && npm run dev:staging\"",
    "build": "cd backend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "build:frontend:dev": "cd frontend && npm run build:dev",
    "build:frontend:staging": "cd frontend && npm run build:staging",
    "build:frontend:prod": "cd frontend && npm run build:prod",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "start": "cd backend && npm start",
    "start:dev": "cd backend && npm run start:dev",
    "start:staging": "cd backend && npm run start:staging",
    "start:prod": "cd backend && npm run start:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
